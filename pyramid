# cs-50-week-1-projects
#include <stdio.h>
#include <cs50.h>

void print_row(int b){
   for (int i= 0 ; i < b ; i++){
         printf("#");
      }
      printf("\n");
      }
int main(void){
   int h ;
do{
      h = get_int("What is the height of the pyrmied : ");
  }while(h < 1);
  for(int i = 0 ; i < h ; i++){
      print_row(i+1);
  }
  }

